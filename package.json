{
  "name": "swagger-client-autogen",
  "version": "0.0.3",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup && yarn postbuild",
    "postbuild": "cp -r src/templates dist/",
    "build:watch": "tsup --watch",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "api-client",
    "codegen",
    "tanstack-query",
    "react-query",
    "cli",
    "code-generation",
    "api"
  ],
  "author": "dhlab-fe",
  "license": "MIT",
  "homepage": "https://github.com/dhlab-org/swagger-client-autogen#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhlab-org/swagger-client-autogen.git"
  },
  "bugs": {
    "url": "https://github.com/dhlab-org/swagger-client-autogen/issues"
  },
  "description": "Swagger/OpenAPI에서 TypeScript API 클라이언트 코드를 자동 생성하는 CLI 도구",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "swagger-client-autogen": "./dist/scripts/cli.js"
  },
  "files": ["dist/"],
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "yarn@4.7.0",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "es-toolkit": "^1.39.8",
    "immer": "^10.1.1",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "picocolors": "^1.1.1",
    "swagger-typescript-api": "^13.2.7",
    "ts-pattern": "^5.8.0",
    "ts-to-zod": "^3.15.0",
    "tsx": "^4.20.3"
  }
}
